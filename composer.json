{
    "name": "tpgwidget/data",
    "type": "library",
    "description": "Composer package to improve TPG API results",
    "keywords": [
        "tpg", "geneva", "open-data", "tpgwidget"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Ettlin",
            "homepage": "https://www.nicolapps.ch/",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tpgwidget/data"
        }
    ],
    "require": {
        "php" : "~7.1|^8.0",
        "colinodell/json5": "^2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TPGwidget\\Data\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPGwidget\\Data\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
