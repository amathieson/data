{
    "name": "tpgwidget/data",
    "type": "library",
    "description": "Composer package to improve TPG API results",
    "keywords": [
        "tpg", "geneva", "open-data", "tpgwidget"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Ettlin",
            "homepage": "https://www.nicolapps.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1",
        "colinodell/json5": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TPGwidget\\Data\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPGwidget\\Data\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
